{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "workbench.iconTheme": "material-icon-theme",
  "java.configuration.checkProjectSettingsExclusions": false,
  "workbench.startupEditor": "newUntitledFile",
  "editor.wordWrap": "on",
  "window.zoomLevel": 0,
  "prettier.singleQuote": true,
  "prettier.jsxSingleQuote": true,
  "prettier.proseWrap": "never",
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    }
  ],
  "eslint.autoFixOnSave": true,
  "editor.formatOnSave": true,
  "editor.rulers": [80, 100],
  "vim.easymotion": true,
  "vim.easymotionMarkerFontFamily": "monospace",
  "vim.easymotionMarkerFontWeight": "bold",
  "vim.easymotionMarkerBackgroundColor": "black",
  "vim.easymotionMarkerForegroundColorOneChar": "#ff00a5",
  "vim.easymotionMarkerFontSize": "16",
  "vim.easymotionMarkerHeight": 14,
  "vim.easymotionMarkerYOffset": 3,
  "vim.report": 1,
  "vim.sneak": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vim.camelCaseMotion.enable": true,
  "workbench.colorTheme": "Github"
}
